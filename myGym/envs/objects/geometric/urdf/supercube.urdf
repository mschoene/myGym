<?xml version="1.0" ?>
<robot name="supercube.urdf">
  <link name="baseLink">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1"/> <!-- You can adjust the mass as needed -->
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <!-- Define the surface geometry for the super cube -->
        <mesh filename="./obj/supercube.obj" scale="0.1 0.1 0.1"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <!-- Define the collision geometry for the super cube -->
        <mesh filename="./obj/supercube.obj" scale="0.1 0.1 0.1"/>
      </geometry>
    </collision>
  </link>
  <!-- Define the heavier cube -->
  <link name="heavierCube">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="10"/> <!-- Adjust the mass of the heavier cube as needed -->
      <inertia ixx="0.002" ixy="0" ixz="0" iyy="0.002" iyz="0" izz="0.002"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <!-- Define the visual geometry for the heavier cube -->
        <box size="0.1 0.1 0.1"/>
      </geometry>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <!-- Define the collision geometry for the heavier cube -->
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>
  </link>
  <!-- Define the joint between the baseLink and the heavierCube -->
  <joint name="heavierCubeJoint" type="fixed">
    <parent link="baseLink"/>
    <child link="heavierCube"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>
</robot>
