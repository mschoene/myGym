<robot name="cube.urdf">
  <link name="baseLink">
    <contact>
      <!-- <lateral_friction value="1.0"/>
      <inertia_scaling value="3.0"/> -->
      <!-- <friction_anchor/> -->
      <!-- <lateral_friction value="1.3"/> -->
      <!-- <rolling_friction value="1.3"/> -->
      <!-- <lateral_friction value="20"/> -->
      <stiffness value="300"/>
      <!-- <damping value="10"/> -->
      <!-- <damping value="20"/> -->
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="-0.005388839478213316 0.01574685150422326 0.024303490688100893"/>     <!-- shift by 0.05 along x axis (edge of cube since size is 0.1) -->
       <mass value="1.0"/>
