    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
				<mesh filename="./obj/cube.obj" scale="0.1 0.1 0.1"/>
      </geometry>
       <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
	<mesh filename="./obj/cube.obj" scale=".1 .1 .1"/>
      </geometry>
    </collision>
  </link>
